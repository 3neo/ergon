    <parameter name="id" class="java.lang.Integer"/>
    <query language="SQL"><![CDATA[SELECT
    js.first_name AS jobSeekerFirstName,
    js.last_name AS jobSeekerLastName,
    js.address AS jobSeekerAddress,
    js.email AS jobSeekerEmail,
    s.name AS competenceName,
    s.description AS competenceDescription,
    TO_CHAR(ex.start_date, 'YYYY') AS experienceAnneDebut,
    TO_CHAR(ex.end_date, 'YYYY') AS experienceAnneFin,
    ex.job_title AS experienceTitre,
    c.name AS entrepriseNom,
    c.address AS entrepriseAddress,
    TO_CHAR(edu.start_date, 'YYYY') AS educationAnneDebut,
    TO_CHAR(edu.end_date, 'YYYY') AS educationAnneFin,
    edu.degree AS educationNiveau,
    edu.institution AS educationUniversite,
    edu.field_of_study AS educationDiplome
FROM job_seeker js
inner join cv c2 on c2.jobseeker_id = js.id
INNER JOIN skill s ON s.jobseeker_id = js.id
INNER JOIN experience ex ON ex.job_seeker_id = js.id
INNER JOIN education edu ON edu.jobseeker_id = js.id
INNER JOIN company c ON c.id = ex.company_id
WHERE c2.id = $P{id}
]]></query>
